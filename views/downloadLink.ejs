<!DOCTYPE HTML>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>汇邻优店下载</title>
    <script type="text/javascript">
        var browser = {
            versions: function() {
                var u = navigator.userAgent, app = navigator.appVersion;
                return {//移动终端浏览器版本信息
                    trident: u.indexOf('Trident') > -1, //IE内核
                    presto: u.indexOf('Presto') > -1, //opera内核
                    webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
                    gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1, //火狐内核
                    mobile: !!u.match(/AppleWebKit.*Mobile.*/) || !!u.match(/AppleWebKit/), //是否为移动终端
                    ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
                    android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, //android终端或者uc浏览器
                    iPhone: u.indexOf('iPhone') > -1 || u.indexOf('Mac') > -1, //是否为iPhone或者QQHD浏览器
                    iPad: u.indexOf('iPad') > -1, //是否iPad
                    webApp: u.indexOf('Safari') == -1, //是否web应该程序，没有头部与底部
                    xiaomi: u.indexOf('xiaomi') > -1 || u.indexOf('XiaoMi') > -1 || u.indexOf('Miui') > -1 || u.indexOf('MI') > -1,
                    huawei: u.indexOf('HUAWEI') > -1 || u.indexOf('huawei'),
                    oppo: u.indexOf('OPPO') > -1 || u.includes('Oppo') > -1,
                    vivo: u.indexOf('Vivo') > -1 || u.indexOf('vivo') > -1,
                    meizu: u.indexOf('MZ') > -1,
                    samsung: u.indexOf('SAMSUNG') > -1 || u.indexOf('Samsung') > -1 || u.indexOf('SM') > -1,
                    smartisan: u.indexOf('') > -1,
                    lenove: u.indexOf('lenove') > -1,
                    letv: u.indexOf('letv') > -1,
                };
            }(),
            language: (navigator.browserLanguage || navigator.language).toLowerCase()
        }

        if (browser.versions.ios || browser.versions.iPhone || browser.versions.iPad) {
            window.location="https://itunes.apple.com/cn/app/id1224852246";  /*appstore iOS 下载地址*/
        }
        else if (browser.versions.android && browser.versions.xiaomi) {
            window.location="http://app.mi.com/details?id=com.hlife";/*安卓 下载地址*/
        }
        else if (browser.versions.android && browser.versions.huawei) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else if (browser.versions.android && browser.versions.oppo) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else if (browser.versions.android && browser.versions.vivo) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else if (browser.versions.android && browser.versions.meizu) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else if (browser.versions.android && browser.versions.samsung) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else if (browser.versions.android && browser.versions.smartisan) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else if (browser.versions.android && browser.versions.lenove) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else if (browser.versions.android && browser.versions.letv) {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
        else {
            window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.hlife"
        }
    </script>
</head>
